  //   setBook(entry.book);
  //   setShow(entry.show);
  //   // setStartDate(entry.startDate);
  //   // setEndDate(entry.endDate);
  // };

  const handleClick = e => {
    e.preventDefault();
    setNumber(input);
    setInput('');
  };

  const handleCustomerClick = e => {
    e.preventDefault();
    setName(customerInput);
    setCustomerInput('');
  };
  const handleSubmit = e => {
    e.preventDefault();
    dispatch(
      bookingData({
        _id: id,
        name,
        number,
        location,
        show,
        driver,
        book,
        date,
        // startDate,
        // endDate,
      })
    );
  };
